<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <packageSources>
        <add key="whenfresh.github.com" value="https://nuget.pkg.github.com/whenfresh/index.json" />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json"/>
    </packageSources>
    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
        <packageSource key="whenfresh.github.com">
            <package pattern="WhenFresh*" />
        </packageSource>
    </packageSourceMapping>
</configuration>